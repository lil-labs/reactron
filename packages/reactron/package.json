{
  "type": "module",
  "name": "@lil-labs/reactron",
  "version": "0.0.18",
  "description": "⚡ React + Electron ⚡",
  "repository": {
    "type": "git",
    "url": "https://github.com/lil-labs/reactron.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "electron",
    "desktop",
    "vite",
    "remix",
    "react-router",
    "desktop",
    "typescript"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "main": "dist/config.js",
  "bin": {
    "reactron": "bin/reactron.js"
  },
  "scripts": {
    "prebuild": "rimraf bin dist",
    "build": "rollup -c",
    "postbuild": "tsc lib/config.ts --outFile dist/config.js --declaration --emitDeclarationOnly",
    "prepublishOnly": "NODE_ENV=production pnpm build"
  },
  "dependencies": {
    "commander": "13.0.0",
    "execa": "9.5.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "tslib": "^2.8.1"
  }
}
